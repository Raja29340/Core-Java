import java.util.Arrays;

class HelloWorld {
    public static void main(String[] args) 
    {
        int a[] = new int[5];
        a[0] = 12;
        a[1] = 24;
        a[2] = 34;
        a[3] = 11;
        a[4] = 33;
        
        System.out.println(Arrays.binarySearch(a, 34));
        

    }
        
}
